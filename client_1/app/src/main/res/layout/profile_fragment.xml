<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_address_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_lname_title"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_below="@+id/tv_address_title"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/tv_username_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_below="@+id/tv_username_title"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_alignLeft="@+id/tv_lname_title" />

    <TextView
        android:id="@+id/tv_lname_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_marginTop="121dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_alignLeft="@+id/tv_lname_title" />

    <TextView
        android:id="@+id/tv_lname_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="102dp"
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_light_default"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/tv_fname_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv_fname_data"
        android:layout_alignStart="@+id/tv_lname_title"
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_light_default"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_alignLeft="@+id/tv_lname_title" />

    <TextView
        android:id="@+id/tv_username_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_marginTop="172dp"
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_light_default"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_alignLeft="@+id/tv_lname_title" />

    <TextView
        android:id="@+id/tv_address_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_centerVertical="true"
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_light_default"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_alignLeft="@+id/tv_lname_title" />

    <TextView
        android:id="@+id/tv_fname_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tv_lname_title"
        android:layout_alignStart="@+id/tv_lname_title"
        android:layout_marginBottom="-73dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:layout_alignLeft="@+id/tv_lname_title" />
</RelativeLayout>